{
	"name": "@parsons/ti-preview",
	"version": "0.0.11",
	"description": "Mini HTML/CSS editor / preview for the Typography & Interaction class at Parsons",
	"author": "Rijk van Zanten <rijkvanzanten@me.com>",
	"license": "MIT",
	"main": "ti-preview.js",
	"module": "ti-preview.js",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/typography-interaction-2425/ti-preview"
	},
	"scripts": {
		"build": "rimraf dist && tsc && rollup -c",
		"build:watch": "rimraf dist && tsc --watch",
