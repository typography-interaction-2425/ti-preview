		"add": "2.0.6",
		"p": "0.2.0",
		"rimraf": "6.0.1",
		"rollup": "4.52.4",
		"rollup-plugin-summary": "3.0.1",
		"ts-lit-plugin": "2.0.2",
		"typescript": "5.9.3"
	},
	"dependencies": {
		"@codemirror/lang-html": "6.4.11",
